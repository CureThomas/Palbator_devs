<launch>

    <include file="$(find speechToTextPalbator)/launch/voice_control.launch" />

    <include file="$(find connectionManager)/launch/connectionManager.launch" />


    <arg name="config" default="$(find HriManager)/config/config_HRI.yaml" doc="Path to parameter config file"/>


    <node
        name="HRIManager"
        pkg="HriManager"
        type="HRIManager.py"
        output="screen">
        <rosparam file="$(arg config)"/>
    </node>

    <node
        name="multiplexer_tablet"
        pkg="HriManager"
        type="multiplexer.py"
        output="screen" />

        
